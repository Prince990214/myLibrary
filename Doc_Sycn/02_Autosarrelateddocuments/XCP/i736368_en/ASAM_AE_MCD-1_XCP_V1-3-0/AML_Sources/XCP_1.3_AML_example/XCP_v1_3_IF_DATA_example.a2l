    /begin PROTOCOL_LAYER

      0x0103                     /* XCP protocol layer version */
      0x00FA                     /* T1 [ms] */
      0x00FA                     /* T2 [ms] */
      0x0000                     /* T3 [ms] */
      0x0000                     /* T4 [ms] */
      0x0000                     /* T5 [ms] */
      0x0000                     /* T6 [ms] */
      0x0000                     /* T7 [ms] */
