      SLAVE                      /* Slave Block Mode supported */
      MASTER                     /* Master Block Mode supported */
      0x2B                       /* MAX_BS */
      0x02                       /* MIN_ST */

    /end PROTOCOL_LAYER

    /begin DAQ

      DYNAMIC                    /* DAQ_CONFIG_TYPE */
      0xFFFF                     /* MAX_DAQ */
      0x0003                     /* MAX_EVENT_CHANNEL */
      0x00                       /* MIN_DAQ */

      OPTIMISATION_TYPE_DEFAULT
      ADDRESS_EXTENSION_FREE
      IDENTIFICATION_FIELD_TYPE_ABSOLUTE
      GRANULARITY_ODT_ENTRY_SIZE_DAQ_BYTE

      0x04                       /* MAX_ODT_ENTRY_SIZE_DAQ */

      OVERLOAD_INDICATION_EVENT

      CORE_LOAD_MAX_TOTAL  0.08

      /begin CORE_LOAD_MAX
        1                        /* CORE_NR: core reference number */
        0.06                     /* CORE_LOAD_MAX: max load of core(CORE_NR) */
      /end CORE_LOAD_MAX

      /begin CORE_LOAD_MAX
        2                        /* CORE_NR: core reference number */
        0.05                     /* CORE_LOAD_MAX: max load of core(CORE_NR) */
      /end CORE_LOAD_MAX

      /begin CORE_LOAD_MAX
        3                        /* CORE_NR: core reference number */
        0.0025                   /* CORE_LOAD_MAX: max load of core(CORE_NR) */
      /end CORE_LOAD_MAX

      /begin DAQ_MEMORY_CONSUMPTION
        0x000032c5               /* DAQ_MEMORY_LIMIT: in Elements[AG] */
        0x0030                   /* DAQ_SIZE: number of elements[AG] per DAQ list */
        0x0010                   /* ODT_SIZE: number of elements[AG] per ODT */
        0x0005                   /* ODT_ENTRY_SIZE: number of elements[AG] per ODT_entry */
        0x0001                   /* ODT_DAQ_BUFFER_ELEMENT_SIZE: number of */
                                 /* payload elements[AG]*factor = sizeof(send buffer)[AG] */
        0x0002                   /* ODT_STIM_BUFFER_ELEMENT_SIZE: number of */
                                 /* payload elements[AG]*factor = sizeof(receive buffer)[AG] */

        /begin BUFFER_RESERVE
          20                     /* ODT_DAQ_BUFFER_ELEMENT_RESERVE  in % of */
                                 /* ODT_DAQ_BUFFER_ELEMENT_SIZE */
          40                     /* ODT_STIM_BUFFER_ELEMENT_RESERVE in % of */
                                 /* ODT_STIM_BUFFER_ELEMENT_SIZE */
        /end BUFFER_RESERVE

      /end DAQ_MEMORY_CONSUMPTION

      /begin EVENT
        "segment synchronous"    /* EVENT_CHANNEL_NAME */
        "ssyn"                   /* EVENT_CHANNEL_SHORT_NAME */
        0                        /* EVENT_CHANNEL_NUMBER */
        DAQ
        1                        /* MAX_DAQ_LIST */
        0                        /* EVENT_CHANNEL_TIME_CYCLE */
        6                        /* EVENT_CHANNEL_TIME_UNIT */
        7                        /* EVENT_CHANNEL_PRIORITY */

        /begin MIN_CYCLE_TIME
          2                      /* EVENT_CHANNEL_TIME_CYCLE */
          6                      /* EVENT_CHANNEL_TIME_UNIT */
        /end MIN_CYCLE_TIME

        /begin BUFFER_RESERVE_EVENT
          30                     /* ODT_DAQ_BUFFER_ELEMENT_RESERVE  in % of */
                                 /* ODT_DAQ_BUFFER_ELEMENT_SIZE */
          60                     /* ODT_STIM_BUFFER_ELEMENT_RESERVE in % of */
                                 /* ODT_STIM_BUFFER_ELEMENT_SIZE */
        /end BUFFER_RESERVE_EVENT

        /begin CPU_LOAD_CONSUMPTION_DAQ
          1E-6                   /* DAQ_FACTOR */
          1E-6                   /* ODT_FACTOR */
          300E-9                 /* ODT_ENTRY_FACTOR */
          /begin ODT_ENTRY_SIZE_FACTOR_TABLE
            1                    /* SIZE */
            150E-9               /* SIZE_FACTOR */
          /end ODT_ENTRY_SIZE_FACTOR_TABLE
          /begin CORE_LOAD_EP
            1                    /* CORE_NR: core reference number */
            0.025                /* CORE_LOAD_EP_MAX: max load of this event part */
          /end CORE_LOAD_EP
        /end CPU_LOAD_CONSUMPTION_DAQ

        /begin CPU_LOAD_CONSUMPTION_QUEUE
          5E-6                   /* ODT_FACTOR */
          300E-9                 /* ODT_ELEMENT_LOAD: length in elements[AG] */
          /begin CORE_LOAD_EP
            1                    /* CORE_NR: core reference number */
            0.025                /* CORE_LOAD_EP_MAX: max load of this event part */
          /end CORE_LOAD_EP
        /end CPU_LOAD_CONSUMPTION_QUEUE
      /end EVENT

      /begin EVENT
        "10ms time synchronous"  /* EVENT_CHANNEL_NAME */
        "10ms"                   /* EVENT_CHANNEL_SHORT_NAME */
        1                        /* EVENT_CHANNEL_NUMBER */
        DAQ
        1                        /* MAX_DAQ_LIST */
        10                       /* EVENT_CHANNEL_TIME_CYCLE */
        6                        /* EVENT_CHANNEL_TIME_UNIT */
        6                        /* EVENT_CHANNEL_PRIORITY */
        /begin CPU_LOAD_CONSUMPTION_DAQ
          1E-6                   /* DAQ_FACTOR */
          1E-6                   /* ODT_FACTOR */
          300E-9                 /* ODT_ENTRY_FACTOR */
          /begin ODT_ENTRY_SIZE_FACTOR_TABLE
            1                    /* SIZE */
            150E-9               /* SIZE_FACTOR */
          /end ODT_ENTRY_SIZE_FACTOR_TABLE
          /begin CORE_LOAD_EP
            2                    /* CORE_NR: core reference number */
            0.025                /* CORE_LOAD_EP_MAX: max load of this event part */
          /end CORE_LOAD_EP
        /end CPU_LOAD_CONSUMPTION_DAQ

        /begin CPU_LOAD_CONSUMPTION_QUEUE
          5E-6                   /* ODT_FACTOR */
          300E-9                 /* ODT_ELEMENT_LOAD ,length in elements[AG] */
          /begin CORE_LOAD_EP
            2                    /* CORE_NR: core reference number */
            0.025                /* CORE_LOAD_EP_MAX: max load of this event part */
          /end CORE_LOAD_EP
        /end CPU_LOAD_CONSUMPTION_QUEUE
      /end EVENT

      /begin EVENT
        "100ms time synchronous" /* EVENT_CHANNEL_NAME */
        "100ms"                  /* EVENT_CHANNEL_SHORT_NAME */
        2                        /* EVENT_CHANNEL_NUMBER */
        DAQ                      /* EVENT_CHANNEL_TYPE */
        1                        /* MAX_DAQ_LIST */
        100                      /* EVENT_CHANNEL_TIME_CYCLE */
        6                        /* EVENT_CHANNEL_TIME_UNIT */
        5                        /* EVENT_CHANNEL_PRIORITY */
        /begin CPU_LOAD_CONSUMPTION_DAQ
          1E-6                   /* DAQ_FACTOR */
          1E-6                   /* ODT_FACTOR */
          300E-9                 /* ODT_ENTRY_FACTOR */
          /begin ODT_ENTRY_SIZE_FACTOR_TABLE
            1                    /* SIZE */
            150E-9               /* SIZE_FACTOR */
          /end ODT_ENTRY_SIZE_FACTOR_TABLE
          /begin CORE_LOAD_EP
            3                    /* CORE_NR: core reference number */
            0.0025               /* CORE_LOAD_EP_MAX: max load of this event part */
          /end CORE_LOAD_EP
        /end CPU_LOAD_CONSUMPTION_DAQ

        /begin CPU_LOAD_CONSUMPTION_QUEUE
          5E-6                   /* ODT_FACTOR */
          300E-9                 /* ODT_ELEMENT_LOAD ,length in elements[AG] */
          /begin CORE_LOAD_EP
            3                    /* CORE_NR: core reference number */
            0.0025               /* CORE_LOAD_EP_MAX: max load of this event part */
          /end CORE_LOAD_EP
        /end CPU_LOAD_CONSUMPTION_QUEUE
      /end EVENT

    /end DAQ

    TRANSPORT_LAYER_INSTANCE "Calibration CAN (APPL)"

  /end XCP_ON_CAN

